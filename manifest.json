{
    "name": "Leetcode format",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Format code on leetcode",
    "homepage_url": "https://madhur.co.in",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "js": ["codemirror.js", "content.js"],
            "matches": ["https://*.leetcode.com/*"]
        }
    ],
    "web_accessible_resources": ["script.js"]
}
